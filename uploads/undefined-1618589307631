#include<bits/stdc++.h>
using namespace std;

long long arr[100][100],dp1[100][100],dp2[100][100];

int main()
{
    int n,m,i,j,ii,jj,k;
    double ans,x,y,z;

    scanf("%d %d",&n,&m);

    ans = 1<<30;
    for(i=1;i<=n;i++){
        for(j=1;j<=m;j++){
            scanf("%lld",&arr[i][j]);
        }
    }
    ans = 1<<30;
    for(i=1;i<=n;i++){
        for(j=1;j<=m;j++){
            z = 0;
            for(k=1;k<=n;k++){
                z += abs(arr[i][j]-arr[k][j]);
            }
            for(k=1;k<=m;k++){
                z += abs(arr[i][j]-arr[i][k]);
            }

            z = z/(n+m-2);
            if(ans > z){
                ans = z;
                ii = i;
                jj = j;
            }
        }
    }
    
    //printf("%lf\n",ans);
    printf("%lld\n",arr[ii][jj]);
    return 0;
}
/*
3 4
8 7 43 12
90 13 22 17
99 12 14 16

*/